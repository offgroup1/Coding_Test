      "source": [
        "def solution(numbers):\n",
        "    answer = ''\n",
        "    numbers = [str(x) for x in numbers]\n",
        "    numbers.sort(key = lambda x: (x*4)[:4], reverse= True)\n",
        "    if numbers[0] == '0':\n",
        "        answer = '0'\n",
        "    else:\n",
        "        answer += ''.join(numbers)\n",
        "    return answer"
      ]
    }
  ]
